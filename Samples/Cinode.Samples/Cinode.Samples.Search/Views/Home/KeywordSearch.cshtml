@model KeywordSearchModel
@{
    ViewData["Title"] = "Keyword - Search";
}
@if (Model != null)
{
    <h1>@Model.Term</h1>

    if (Model.Keywords?.Count > 0)
    {
        <ul>
            @foreach (var keyword in Model.Keywords)
            {
                <li>@keyword.MasterSynonym</li>
            }
        </ul>
    }
    else
    {
        <h2>Hittade inget :/</h2>
    }
}
else
{
    <h1>Något gick fel</h1>
}

